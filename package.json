    "class-validator": "^0.14.2",
    "class-validator-jsonschema": "^5.0.2",
    "dto-mapped-types": "^0.0.7",
    "json-schema-to-zod": "^2.6.1",
    "lodash": "^4.17.21",
    "next": "^15.3.2",
    "nextra": "^4.2.17",
    "nextra-theme-docs": "^4.2.17",
    "openai": "^4.103.0",
    "react": "^19.1.0",
