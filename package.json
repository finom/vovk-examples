{
  "name": "vovk-examples",
  "version": "3.0.17",
  "description": "Vovk examples",
  "private": true,
  "license": "MIT",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "npm run lint",
    "upgrade": "npx npm-check-updates -u && npm i",
    "predev": "npm run update-mdx-github-codeblocks",
    "dev": "vovk dev --next-dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "patch": "npm t && npm version patch",
    "minor": "npm t && npm version minor",
    "BREAKING-major": "npm t && npm version major",
    "postversion": "npm run bundle && npm publish ./dist && git push && git push --tags",
    "generate": "vovk generate",
    "bundle": "NODE_ENV=production vovk bundle",
    "prebuild": "vovk generate",
    "postbuild": "pagefind --site .next/server/app --output-path public/_pagefind",
    "vercel-build": "npm run build",
    "update-mdx-github-codeblocks": "node --experimental-strip-types ./scripts/updateMDXFilesWithGithubContent.ts"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "printWidth": 120
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/finom/vovk-examples.git"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.72",
    "@ai-sdk/react": "^2.0.102",
    "@hookform/resolvers": "^5.2.2",
    "@nestjs/common": "^11.1.9",
    "@nestjs/mapped-types": "^2.1.0",
    "@scalar/api-reference-react": "^0.8.6",
    "@tanstack/react-query": "^5.90.11",
    "@valibot/to-json-schema": "^1.3.0",
    "ai": "5.0.102",
    "ajv": "^8.17.1",
    "ajv-errors": "^3.0.0",
    "ajv-formats": "^3.0.1",
    "arktype": "^2.1.27",
    "bright": "^1.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "class-validator-jsonschema": "^5.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dto-mapped-types": "^0.0.7",
    "lodash": "^4.17.21",
    "lucide-react": "^0.554.0",
    "next": "^16.0.4",
    "nextra": "^4.6.0",
    "nextra-theme-docs": "^4.6.0",
    "openai": "^6.9.1",
    "openapi3-ts": "^4.5.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-hook-form": "^7.66.1",
    "react-loading-skeleton": "^3.5.0",
    "react-to-string": "^0.1.1",
    "tailwind-merge": "^3.4.0",
    "tsdown": "^0.16.7",
    "valibot": "^1.2.0",
    "vovk": "^3.0.0-beta.50",
    "vovk-ajv": "^0.0.0-beta.4",
    "vovk-apis": "^0.0.1-draft.0",
    "vovk-client": "^0.0.4-beta.4",
    "vovk-dto": "^0.0.1-beta.13",
    "vovk-yup": "^0.0.1-beta.13",
    "vovk-zod": "^1.0.0-beta.14",
    "yup": "^1.7.1",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@types/lodash": "^4.17.21",
    "@types/node": "^24",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.4",
    "pagefind": "^1.4.0",
    "postcss": "^8",
    "prettier": "^3.6.2",
    "reflect-metadata": "^0.2.2",
    "tailwindcss": "^4.1.17",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3",
    "vovk-cli": "^0.0.1-beta.49",
    "vovk-python": "^0.0.1-beta.4",
    "vovk-rust": "^0.0.1-beta.4"
  }
}
