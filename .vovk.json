{
  "BasicController": {
    "_controllerName": "BasicController",
    "_prefix": "basic",
    "_handlers": {
      "getHello": {
        "path": "greeting",
        "httpMethod": "GET"
      }
    }
  },
  "BasicControllerWithService": {
    "_controllerName": "BasicControllerWithService",
    "_prefix": "basic-with-service",
    "_handlers": {
      "getHello": {
        "path": "greeting",
        "httpMethod": "GET"
      }
    }
  },
  "StreamController": {
    "_controllerName": "StreamController",
    "_prefix": "stream",
    "_handlers": {
      "streamTokens": {
        "path": "tokens",
        "httpMethod": "GET"
      }
    }
  },
  "StreamResponseObjectController": {
    "_controllerName": "StreamResponseObjectController",
    "_prefix": "stream-with-object",
    "_handlers": {
      "streamTokens": {
        "path": "tokens",
        "httpMethod": "GET"
      }
    }
  },
  "OpenAiController": {
    "_controllerName": "OpenAiController",
    "_prefix": "openai",
    "_handlers": {
      "createChatCompletion": {
        "path": "chat",
        "httpMethod": "POST"
      }
    }
  },
  "FormController": {
    "_controllerName": "FormController",
    "_prefix": "form",
    "_handlers": {
      "createUser": {
        "clientValidators": {
          "body": {
            "type": "object",
            "properties": {
              "firstName": {
                "type": "string",
                "minLength": 2,
                "maxLength": 20
              },
              "lastName": {
                "type": "string",
                "minLength": 2,
                "maxLength": 20
              },
              "email": {
                "type": "string",
                "format": "email"
              }
            },
            "required": [
              "firstName",
              "lastName",
              "email"
            ],
            "additionalProperties": false,
            "$schema": "http://json-schema.org/draft-07/schema#"
          },
          "query": null
        },
        "path": "create-user",
        "httpMethod": "POST"
      }
    }
  },
  "workers": {
    "WorkerService": {
      "_workerName": "WorkerService",
      "_handlers": {
        "factorize": {}
      }
    },
    "WorkerYieldService": {
      "_workerName": "WorkerYieldService",
      "_handlers": {
        "approximatePi": {
          "isGenerator": true
        }
      }
    }
  }
}