    "FormZodRPC": {
      "rpcModuleName": "FormZodRPC",
      "originalControllerName": "FormZodController",
      "prefix": "form-zod",
      "handlers": {
        "submitForm": {
          "validation": {
            "body": {
              "$schema": "https://json-schema.org/draft/2020-12/schema",
              "type": "object",
              "properties": {
                "email": {
                  "type": "string",
                  "format": "email",
                  "pattern": "^(?!\\.)(?!.*\\.\\.)([A-Za-z0-9_'+\\-\\.]*)[A-Za-z0-9_+-]@([A-Za-z0-9][A-Za-z0-9\\-]*\\.)+[A-Za-z]{2,}$",
                  "description": "User email"
                },
                "resume": {
                  "type": "string",
                  "format": "binary",
                  "contentEncoding": "binary",
                  "description": "Resume file",
                  "examples": [
                    "application/pdf"
                  ]
                },
                "portfolioSamples": {
                  "anyOf": [
                    {
                      "type": "array",
                      "items": {
                        "type": "string",
                        "format": "binary",
                        "contentEncoding": "binary"
                      }
                    },
                    {
                      "type": "string",
                      "format": "binary",
                      "contentEncoding": "binary"
                    }
                  ],
                  "description": "Portfolio samples"
                }
              },
              "required": [
                "email",
                "resume",
                "portfolioSamples"
              ],
              "description": "User object",
              "x-isForm": true
            },
            "params": {
              "$schema": "https://json-schema.org/draft/2020-12/schema",
              "type": "object",
              "properties": {
                "id": {
                  "type": "string",
                  "format": "uuid",
                  "pattern": "^([0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[1-8][0-9a-fA-F]{3}-[89abAB][0-9a-fA-F]{3}-[0-9a-fA-F]{12}|00000000-0000-0000-0000-000000000000|ffffffff-ffff-ffff-ffff-ffffffffffff)$",
                  "description": "User ID"
                }
              },
              "required": [
                "id"
              ]
            },
            "output": {
              "$schema": "https://json-schema.org/draft/2020-12/schema",
              "type": "object",
              "properties": {
                "email": {
                  "type": "string",
                  "format": "email",
                  "pattern": "^(?!\\.)(?!.*\\.\\.)([A-Za-z0-9_'+\\-\\.]*)[A-Za-z0-9_+-]@([A-Za-z0-9][A-Za-z0-9\\-]*\\.)+[A-Za-z]{2,}$",
                  "description": "User email"
                },
                "resume": {
                  "type": "object",
                  "properties": {
                    "name": {
                      "type": "string",
                      "description": "Resume file name",
                      "examples": [
                        "resume.pdf"
                      ]
                    },
                    "size": {
                      "type": "number",
                      "minimum": 0,
                      "description": "Resume file size"
                    },
                    "type": {
                      "type": "string",
                      "description": "Resume file type",
                      "examples": [
                        "application/pdf"
                      ]
                    }
                  },
                  "required": [
                    "name",
                    "size",
                    "type"
                  ]
                },
                "portfolioSamples": {
                  "type": "array",
                  "items": {
                    "type": "object",
                    "properties": {
                      "name": {
                        "type": "string",
                        "description": "Portfolio sample file name",
                        "examples": [
                          "portfolio.zip"
                        ]
                      },
                      "size": {
                        "type": "number",
                        "minimum": 0,
                        "description": "Portfolio sample file size"
                      },
                      "type": {
                        "type": "string",
                        "description": "Portfolio sample file type",
                        "examples": [
                          "application/zip"
                        ]
                      }
                    },
                    "required": [
                      "name",
                      "size",
                      "type"
                    ]
                  },
                  "description": "Array of portfolio sample files"
                }
              },
              "required": [
                "email",
                "resume",
                "portfolioSamples"
              ],
              "description": "Response object"
            }
          },
          "operationObject": {
            "summary": "Submit form (Zod)",
            "description": "Submit form with Zod validation"
          },
          "path": "{id}",
          "httpMethod": "POST"
        }
      }
    }
  }
